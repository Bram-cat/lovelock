{
  "cli": {
    "version": ">= 14.2.0",
    "requireCommit": false,
    "appVersionSource": "local"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "buildConfiguration": "Debug"
      }
    },
    "preview": {
      "android": {
        "buildType": "apk",
        "image": "ubuntu-22.04-jdk-17-ndk-r26b"
      },
      "ios": {
        "buildConfiguration": "Release"
      },
      "distribution": "internal",
      "env": {
        "NODE_ENV": "production",
        "EXPO_USE_FABRIC": "0",
        "RCT_NEW_ARCH_ENABLED": "0"
      },
      "cache": {
        "disabled": true
      },
      "node": "18.20.0"
    },
    "production": {
      "android": {
        "buildType": "app-bundle",
        "gradleCommand": ":app:bundleRelease",
        "env": {
          "EXPO_USE_FABRIC": "0",
          "RCT_NEW_ARCH_ENABLED": "0"
        }
      },
      "ios": {
        "buildConfiguration": "Release",
        "env": {
          "EXPO_USE_FABRIC": "0",
          "RCT_NEW_ARCH_ENABLED": "0"
        }
      }
    }
  },
  "submit": {
    "production": {}
  }
}
