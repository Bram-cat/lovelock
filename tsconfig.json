{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "app/*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "app/(auth)/sign-in.jsx",
    "app/index.jsx",
    "app/_layout.tsx",
    "app/(auth)/sign-up.jsx",
    "app/(auth)/_layout.jsx",
    "app/test-home.jsx",
    "app/simple-layout.jsx",
    "app/numerology.jsx",
    "nativewind-env.d.ts"
  ]
}
