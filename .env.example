# Environment Variables Template
# Copy this file to .env and fill in your actual API keys

# Google Gemini AI Configuration
GOOGLE_AI_API_KEY=your_primary_gemini_api_key_here
BACKUP_GOOGLE_AI_API_KEY=your_backup_gemini_api_key_here
EXPO_PUBLIC_GOOGLE_AI_API_KEY=your_primary_gemini_api_key_here

# OpenAI Configuration
OPENAI_API_SECRET_KEY=your_openai_api_key_here
EXPO_PUBLIC_OPENAI_API_SECRET_KEY=your_openai_api_key_here

# Clerk Authentication
EXPO_PUBLIC_CLERK_PUBLISHABLE_KEY=your_clerk_publishable_key
CLERK_SECRET_KEY=your_clerk_secret_key
CLERK_PLAN_ID=your_clerk_plan_id

# Google OAuth Configuration
EXPO_PUBLIC_GOOGLE_OAUTH_CLIENT_ID=your_google_oauth_client_id
EXPO_PUBLIC_GOOGLE_OAUTH_CLIENT_SECRET=your_google_oauth_client_secret

# Stripe Configuration
STRIPE_SECRET_KEY=your_stripe_secret_key
EXPO_PUBLIC_STRIPE_PUBLISHABLE_KEY=your_stripe_publishable_key
PREMIUM_PRICE_ID=your_premium_price_id
STRIPE_WEBHOOK_SECRET=your_webhook_secret
STRIPE_BACKEND_URL=your_backend_url

# Supabase Configuration
EXPO_PUBLIC_SUPABASE_URL=your_supabase_url
EXPO_PUBLIC_SUPABASE_AUTH_URL=your_supabase_auth_url
EXPO_PUBLIC_SUPABASE_KEY=your_supabase_anon_key
EXPO_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
EXPO_PUBLIC_SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# ProKerala Configuration
PROKERALA_CLIENT_ID=your_prokerala_client_id
PROKERALA_CLIENT_SECRET=your_prokerala_client_secret